;; ForEach on a sequence: processes every element in seq order
(defnode Main)
(defloop test

    [2 4 8 10]
    (ForEach
        (-> 
            (| (Math.Multiply 10) 
               (Log))  ;; => 20, 40, 80, 100
            (| (Math.Multiply 100)
               (Log))  ;; => 200, 400, 800, 1000
        )
    )
)
(schedule Main test)
(run Main 1 1)
